{
  "expo": {
    "name": "popword",
    "slug": "react-native-popword",
    "version": "2.0.0",
    "orientation": "portrait",
    "description": "The Ultimate Dictionary ",
    "icon": "./assets/dictionary.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.nativeApper.popword",
      "permissions": []
    },
    "web": {
      "favicon": "./assets/dictionary.png"
    }
  }
}
